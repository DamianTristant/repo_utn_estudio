Algoritmo or
	Definir aniosAporte, edad Como Entero;
	Escribir "Ingrese su edad:";
	Leer edad;
	Escribir "Ingrese su años de aporte:";
	Leer aniosAporte;
	
	si edad >= 65 o aniosAporte >= 30 Entonces
		Escribir "Usted se puede jubilar";
	SiNo
		Escribir "Usted no cuenta con ninguno de los requisitos";
	FinSi
	
	
FinAlgoritmo
