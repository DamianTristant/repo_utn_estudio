Algoritmo numeroMultiplo
	Definir num1, num2 Como Entero;
	Escribir "Ingrese el primer numero:";
	Leer num1;
	Escribir "Ingrese el segundo numero:";
	Leer num2;
	
	si num1 mod num2 == 0
		Escribir "El num ", num1, " es multiplo de ", num2;
	SiNo
		Escribir "No son multiplos";
	FinSi
	
	
	
FinAlgoritmo
